{
    "name": "GAMBA",
    "description": "Generalised AI Browser Assistant",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "GAMBA_icon.png"
    }
}